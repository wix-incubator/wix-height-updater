{
  "name": "wix-height-updater",
  "version": "1.0.1",
  "homepage": "",
  "author": {
    "name": "Eran Shabi",
    "email": "erans@wix.com",
    "url": ""
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yoshi start --entry-point=./test/mock/start-fake-server.js",
    "build": ":",
    "precommit": "yoshi lint",
    "pretest": "yoshi build",
    "test": "npm run test:unit & npm run test:e2e",
    "test:unit": "yoshi test --jest",
    "test:e2e": "yoshi test --protractor",
    "posttest": "yoshi lint",
    "release": "yoshi release"
  },
  "yoshi": {
    "entry": "./app.tsx",
    "separateCss": false
  },
  "devDependencies": {
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "express": "^4.13.4",
    "express-session": "^1.14.2",
    "husky": "^0.13.4",
    "jsdom-global": "^2.1.0",
    "react-test-renderer": "^15.5.4",
    "require-reload": "^0.2.2",
    "typescript": "^2.4.2",
    "velocity": "^0.7.2",
    "wix-statics-parent": "latest",
    "yoshi": "^3.0.0",
    "yoshi-style-dependencies": "^3.0.0",
    "protractor": "^5.3.2",
    "@types/jest": "^23.1.4",
    "ts-jest": "^23.0.0",
    "tslib": "^1.7.1"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "i18next": "^7.2.1",
    "i18next-xhr-backend": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-i18next": "^3.1.0",
    "wix-axios-config": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/wix-height-updater.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock/fileMock.js"
    },
    "testURL": "http://localhost:5554"
  }
}
