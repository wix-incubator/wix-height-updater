<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>${title}</title>
    </head>
    <body>
        <div data-hook="iframe-container" style="position:relative">
            <script>
                var Wix = {};
                Wix.setHeight = (newHeight) => {
                document.querySelector('[data-hook="iframe-container"]').style.height = newHeight + 'px';
                }
            </script>
        <iframe id="app-iframe" name="app-iframe" style="display:block;width:100%;height:100%;overflow:hidden;position:absolute;"
        src="/"
        ></iframe>
        </div>


    </body>
</html>
